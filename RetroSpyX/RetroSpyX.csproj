<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <BuiltInComInteropSupport>true</BuiltInComInteropSupport>
    <RootNamespace>RetroSpy</RootNamespace>
    <AssemblyName>RetroSpy</AssemblyName>
    <ApplicationIcon>RetroSpy.ico</ApplicationIcon>
    <BaseOutputPath>..\bin</BaseOutputPath>
    <UseWPF>False</UseWPF>
    <NeutralLanguage>en-US</NeutralLanguage>
  </PropertyGroup>
	<PropertyGroup>
		<AssemblyVersion>6.8.1</AssemblyVersion>
		<FileVersion>6.8.1</FileVersion>
		<Version>6.8.1</Version>
		<Copyright>Copyright Â© 2022</Copyright>
		<ApplicationManifest>app.manifest</ApplicationManifest>
		<UseWindowsForms>False</UseWindowsForms>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
	</PropertyGroup>
	<PropertyGroup>
		<CurrentDate>$([System.DateTime]::Now.ToString("ddd MM/dd/yyyy HH:mm:ss.FF"))</CurrentDate>
		<ProduceReferenceAssembly>False</ProduceReferenceAssembly>
	</PropertyGroup>
	<PropertyGroup Condition="$([MSBuild]::IsOSPlatform('Windows'))">
		<DefineConstants>OS_WINDOWS</DefineConstants>
	</PropertyGroup>
